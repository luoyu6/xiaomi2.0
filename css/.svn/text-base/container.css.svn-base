.btn a i {
	display: inline-block;
	width: 42px;
	height: 70px;
	font-size: 37px;
	/*background: red;*/
	position: absolute;
	top: 50%;
	z-index: 99;
	color: #b0b0b0;
	line-height: 70px;
	font-weight: 400;
}

.btn a i:hover {
	background: rgba(0, 0, 0, 0.5);
}

