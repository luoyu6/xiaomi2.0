img {
	width: 100%;
	height: 100%;
}

.nav_bar {
	
	width: 100%;
	background: #fff;
	border-top: 1px solid #e0e0e0;
	border-bottom: 1px solid #e0e0e0;
	-webkit-box-shadow: 0px 5px 5px rgba(0, 0, 0, 0.07);
	box-shadow: 0px 5px 5px rgba(0, 0, 0, 0.07);
}

.ui_wrapper {
	background: gray;
	position: relative;
}

.ui_wrapper ul {
	width: 100%;
	height: 100%;
}

.ui_wrapper li {
	width: 100%;
	height: 100%;
	position: absolute;
	overflow: hidden;
}


/*.ui_wrapper li img {
	width: 100%;
