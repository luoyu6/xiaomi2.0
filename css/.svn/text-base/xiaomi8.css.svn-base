}

.nav_bar {
	height: 60px;
}

.nav_bar h2 {
	margin: 0;
	font-size: 18px;
	font-weight: 400;
	line-height: 60px;
	color: #212121;
	float: left;
}

.nav_bar div {
	line-height: 60px;
}

.nav_bar div.fl span {
	margin: 0 5px;
}

.nav_bar div.fr a {
	margin: 0 5px;
}

.nav_bar div.fl,
.nav_bar div.fr {
	font-size: 12px;
}

.nav_bar .fl a,
.nav_bar .fr a {
	color: #616161;
	text-decoration: none;
}


