}*/

.ui_page {
	position: absolute;
	bottom: 20px;
	left: 50%;
	margin-left: -63px;
}

.ui_page a {
	display: inline-block;
	width: 30px;
	height: 3px;
	background: #888;
	margin: 0 5px;
}

.ui_wrapper ul li {}

.ui_page a.on {
	background: #fff;
}

#section_border .page a.on {
	background: #fff;
}

.section_index {
	min-width: 1190px;
	position: relative;
}

.section_index .txt {
	position: absolute;
	top: 0;
	z-index: 100;
	width: 100%;
	text-align: center;
	padding-top: 100px;
	/*后期需要修改---------------------------------------------*/
}

.txt .price {}

.txt .price i {
	font-style: normal;
}

.z_index3 {
	z-index: 3;
}

.z_index4 {
	z-index: 4;
}


